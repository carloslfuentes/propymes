= form_for(@user_session, url: {action: "create_session"}) do |f|
  .ul
    =@user_session.errors.full_messages.each do |msg|
      .li
        = msg
      %br
  .div{:class=>"field"}
    = f.label :login
    %br
    =f.text_field :login
  .div{:class=>"field"}
    = f.label :password 
    %br
    = f.text_field :password
  .div{:class=>"actions"}
    = f.submit

